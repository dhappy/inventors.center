<script>
$(function() {
  // create subheads in list sections
  $('.list-item-content__title').each(function() {
    var text = $(this).text().split('::');
    for (var newHtml = '', i = 0; i < text.length; i++) {
      newHtml += '<div class="group' + (i+1) + '">' + text[i] + '</div>';
    }
    $(this).html(newHtml);
  });
  
  // notify if no filtered items exist
  var $filter = $('.eventlist-filter');
  if ($filter.length) {
    if (!$('.eventlist--upcoming').length) {
      var filterText = $filter.eq(0).text().trim();
      var filterLabel = filterText.substring(filterText.indexOf('“') + 1, filterText.indexOf('”'));
      console.log(filterText, filterText.indexOf('“'), filterText.indexOf('”'), filterLabel);
      $filter.after('<p><i>No events are scheduled for ' + filterLabel + '.</i> <a href="?">Show all upcoming events</a></p>');
    }
  }
  
  // to add an entire page or a single block into a block on another page
  // replace with page content
  $('.replace-with-page-content').each(function() {
    $this = $(this);
    $parentBlock = $this.closest('.sqs-block');
    var uri = $this.attr('data-uri');
    var blockId = $this.attr('data-block-id');
    var uriSelector;
    if (blockId) {
      uriSelector = uri + " #" + blockId;
    } else {
      uriSelector = uri + " #sections .content";
    }
    console.log('replace', $this, $parentBlock, uri, blockId, uriSelector);
    $('<div>')
      .load(uriSelector, function() {
        // after load
        $(this).insertBefore($parentBlock);
        $parentBlock.remove();
        // init blocks that may need it
        Squarespace.initializeLayoutBlocks(Y);
      });
  });
  /*
  will replace:
  <div class="replace-with-page-content" data-uri="/page-with-content-uri" data-block-id="the-block-id"></div>
  */
